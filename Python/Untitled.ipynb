   "source": [
    "A = [100,250,1000]\n",
    "len_A = len(A)\n",
    "A.sort()\n",
    "for i in range(len_A-2):\n",
    "    add_num = A[i]+A[i+1]\n",
    "    j = i + 1\n",
    "    while add_num > A[j] and j < len_A-1:\n",
    "        A[j] = A[j+1]\n",
    "        j = j + 1\n",
    "    A[j-1] = add_num\n",
    "    A[i] = 0\n",
    "    \n",
    "A"
   ]
